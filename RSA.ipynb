        "from Crypto.PublicKey import RSA\n",
        "\n",
        "private_key = RSA.generate(2048)\n",
        "public_key = private_key.publickey()\n",
        "\n",
        "text = \"Gautam Pala\"\n",
        "cipher = public_key.encrypt(text.encode(\"utf8\"),32)[0]\n",
        "print(cipher)\n",
        "dec = private_key.decrypt(cipher)\n",
        "print(dec)"
